{
	"name": "consistence/sentry",
	"description": "-",
	"license": "MIT",
	"require": {
		"consistence/consistence": "~1.0",
		"doctrine/inflector": "~1.0"
	},
	"require-dev": {
		"php": "~7.1",
		"consistence/coding-standard": "2.4",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "1.0",
		"mikey179/vfsStream": "1.6.5",
		"phing/phing": "2.16.1",
		"phpunit/phpunit": "6.5.12"
	},
	"autoload": {
		"psr-4": { "Consistence\\Sentry\\": [ "src" ] },
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": { "Consistence\\Sentry\\": [ "tests" ] },
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
